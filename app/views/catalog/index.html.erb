<div id="bd">
<div id="listleftcol" class="yui-b <%= sidebar_classes %>">
  <%= render 'search_sidebar' %>
</div>

<div id="mbContentContainer" class="yui-main content main clearfix <%= main_content_classes %>">
  <% unless has_search_parameters? %>
    <%# if there are no input/search related params, display the "home" partial -%>
    <%= render 'home' %>
  <% else %>
    <%= render 'search_results' %>
  <% end %>
</div>
</div>
